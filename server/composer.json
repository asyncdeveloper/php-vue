{
  "name": "php-vue/server",
  "description": "php rest api",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "oluwaseyi",
      "email": "sheyilaaw98@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.4.2",
    "squizlabs/php_codesniffer": "^3.8"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config":{
    "optimize-autoloader":true
  },
  "scripts": {
    "test": "phpunit tests",
    "lint": "phpcbf -v --standard=PSR2 --ignore=vendor ."
  },
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "pecee/simple-router": "^5.4.1.7",
    "illuminate/database": "^10.43",
    "illuminate/pagination": "^10.43",
    "rakit/validation": "^1.4",
    "zircote/swagger-php": "^4.8",
    "doctrine/annotations": "^2.0"
  }
}
